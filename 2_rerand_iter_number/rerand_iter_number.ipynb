   "source": [
    "# Rerandomization iterations mini-note\n",
    "Показывает рост вероятности хотя бы одного ложного срабатывания при росте числа фичей."
   ]
  },
  {
   "cell_type": "code",
   "metadata": {},
   "execution_count": 1,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": "m= 5: P(any false positive)≈0.226\nm=10: P(any false positive)≈0.401\nm=20: P(any false positive)≈0.642\nm=50: P(any false positive)≈0.923\n"
    }
   ],
   "source": [
    "alpha = 0.05\n",
    "for m in [5, 10, 20, 50]:\n",
    "    p_any = 1 - (1-alpha)**m\n",
    "    print(f'm={m:2d}: P(any false positive)≈{p_any:.3f}')"
   ]
  }
 ],
